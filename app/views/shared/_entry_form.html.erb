<%= form_for(@entry) do |f| %>
<%= render 'shared/error_messages', object: f.object %>
<div class="field">
  <%= f.text_field :title,placeholder:"Compose title for new entry..."%>
<%= f.text_area :content, placeholder: "Compose content for new entry..." %>
</div>
<%= f.submit "Post", class: "btn btn-lg btn-primary " %>
<% end %>
